# Claude API Configuration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_PROMPTS_FOLDER="claude_prompts"
# Optional: Customize the agents folder path (defaults to ".claude/agents")
CLAUDE_AGENTS_FOLDER=.claude/agents
# MongoDB Configuration (optional - defaults are set in docker-compose.yml)
MONGODB_URL=mongodb://your_root_user_here:your_root_pw_here@mongodb:27017/claude_workflows?authSource=admin
MONGO_INITDB_ROOT_USERNAME: your_root_user_here
MONGO_INITDB_ROOT_PASSWORD: your_root_pw_here
USE_CLAUDE_MAX_PLAN=true
# Redis Configuration (optional - defaults are set in docker-compose.yml)  
# REDIS_URL=redis://localhost:6379

# Frontend Configuration (optional)
# REACT_APP_API_URL=http://localhost:8000
# REACT_APP_WS_URL=ws://localhost:8000

# Voice Services Configuration (optional - for voice interaction features)
# Picovoice API Key (required for Text-to-Speech)
# Get your free key at: https://console.picovoice.ai/
PICOVOICE_ACCESS_KEY=your_picovoice_access_key_here
REACT_APP_VOICE_API_URL=http://localhost:14300